function buildSilentAuctions($bRemoveAuction){
    _log("==> entering buildSilentAuctions");

    createSilentAuction();

    addPackage("Basket of Books", "Check it out!", "10.00", "2.00", "100.00", "50.00");
    
    addItemsBooks();
    
    setPackageWinner();
    
    setFulFill();
    
    if ($bRemoveAuction){
        removeAuction();
    }
}



