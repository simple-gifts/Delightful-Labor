function removeAuction(){
    _log("==> entering removeAuction");
    _click(_link("nb_more_home"));
    _click(_link("Silent Auction Events[1]"));
    _click(_image("Remove Auction"));
    _expectConfirm("Are you sure you want to remove this auction record?\n\nThis will also remove the auction's associated packages, gift items, and donation records.\n\nThis can not be undone.", true);
}

function addPackage($strPackageName, $strNotes, $strMinBid, $strBidInc, $strBuyItNow, $strReserve){
    _click(_link("nb_more_home"));
    _click(_link("Silent Auction Events[1]"));
    _click(_image("Add new package"));
    _setValue(_textbox("txtPackageName"), $strPackageName);
    _setValue(_textarea("txtPublicNotes"), $strNotes);
    _setValue(_textbox("txtMinBid"), $strMinBid);
    _setValue(_textbox("txtMinBidInc"), $strBidInc);
    _setValue(_textbox("txtBuyItNowAmnt"), $strBuyItNow);
    _setValue(_textbox("txtReserve"), $strReserve);
    _setSelected(_select("ddlBS"), "Spring Simple (template \"Package Info\")");
    _click(_submit("Save"));

    _assertExists(_cell($strPackageName));
}

