
function upgradeTest($strLogInPath){

    _navigateTo($strLogInPath);
    _setValue(_textbox("username"), "admin");
    _setValue(_password("password"), "admin");
    _click(_submit("Log In"));
    _click(_div("login"));
    _assertExists(_div("login"));
    _assert(_isVisible(_div("login")));
    
    _assertExists(_link("upgrade_run"));
    _click(_link("upgrade_run"));
    
    
    _assertExists(_link("here"));
    _assert(_isVisible(_link("here")));
    _assertEqual("here", _getText(_link("here")));
    _assertContainsText("here", _link("here"));
    _click(_link("here"));
    _setValue(_textbox("username"), "admin");
    _setValue(_password("password"), "admin");
    _click(_submit("Log In"));
    _assertExists(_div("Hello, Jane!"));
    _assert(_isVisible(_div("Hello, Jane!")));
    _click(_link("Report Directory"));
    _assertExists(_link("Add new report"));
    _assert(_isVisible(_link("Add new report")));
    _assertEqual("Add new report", _getText(_link("Add new report")));
    _assertContainsText("Add new report", _link("Add new report"));
    _click(_link("Sign Out"));
}

function upgradeDB($strLogInPath){
   _log("==> entering upgradeDB");
   upgradeTest($strLogInPath);
}
